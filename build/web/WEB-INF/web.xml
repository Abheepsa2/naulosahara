<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	 version="3.1">
    <welcome-file-list>
        <welcome-file>Welcome.html</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>donor</servlet-name>
        <servlet-class>sahara</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>donor</servlet-name>
        <url-pattern>/abc</url-pattern>
    </servlet-mapping>
     <servlet>
        <servlet-name>receiver</servlet-name>
        <servlet-class>recSahara</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>receiver</servlet-name>
        <url-pattern>/xyz</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>validate</servlet-name>
        <servlet-class>loginValidation</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>validate</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
      <servlet>
        <servlet-name>finalpage</servlet-name>
        <servlet-class>foodInfo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>finalpage</servlet-name>
        <url-pattern>/received</url-pattern>
    </servlet-mapping>
    <servlet>
     <servlet-name>removing</servlet-name>
        <servlet-class>deleting</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>removing</servlet-name>
        <url-pattern>/DeleteInfo</url-pattern>
    </servlet-mapping>
</web-app>
